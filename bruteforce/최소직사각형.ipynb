   "source": [
    "def solution(sizes):\n",
    "    width = []\n",
    "    length = []\n",
    "    for size in sizes:\n",
    "        width.append(max(size))\n",
    "        length.append(min(size))\n",
    "    answer = max(width) * max(length)\n",
    "    return answer"
   ]
  }
